@model Facebook1.Models.User

@{
    ViewBag.Title = "LogIn";
}

<h2>LogIn</h2>

<form method="post" enctype="multipart/form-data">
    <label>E-Mail</label>
    <input type="text" name="email" />
    <br />
    <label>Password</label>
    <input type="text" name="password" />
    <br />
    <input type="button" value="Enter"/>
</form>